{
  "name": "userviewone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:lint:project": "next lint --dir src",
    "dev:type-check": "tsc --noEmit --project tsconfig.test.json",
    "dev:type-check:project": "tsc --noEmit --project tsconfig.project.json",
    "qa:type-check:cypress": "tsc --noEmit --project cypress/tsconfig.json",
    "qa:lint:cypress": "eslint cypress/e2e/**/*.ts cypress/component/**/*.tsx cypress/support/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev:check:all": "npm run dev:lint:project && npm run dev:type-check:project && npm run test",
    "qa:check:types": "npm run qa:lint:cypress && npm run qa:type-check:cypress",
    "qa:cypress:open": "cypress open",
    "qa:cypress:run": "cypress run"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.7.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^14.5.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "eslint-plugin-cypress": "^5.1.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "typescript": "^5"
  }
}
