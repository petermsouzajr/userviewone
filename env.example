# API Configuration
# Primary environment variables (set these in your environment)
NEXT_PUBLIC_API_BASE_URL=https://jsonplaceholder.typicode.com
NEXT_PUBLIC_API_TIMEOUT=5000
NEXT_PUBLIC_USERS_ENDPOINT=/users
NEXT_PUBLIC_POSTS_ENDPOINT=/posts

# Fallback environment variables (optional - for development/testing)
# These are used if the primary variables are not set
# NEXT_PUBLIC_API_BASE_URL_FALLBACK=https://api.example.com
# NEXT_PUBLIC_API_TIMEOUT_FALLBACK=10000
# NEXT_PUBLIC_USERS_ENDPOINT_FALLBACK=/api/users
# NEXT_PUBLIC_POSTS_ENDPOINT_FALLBACK=/api/posts

# Development Configuration
NODE_ENV=development

# Feature flags
NEXT_PUBLIC_ENABLE_MOCK_DATA=false
NEXT_PUBLIC_ENABLE_DEBUG_LOGGING=true

# App Configuration
# Primary app variables
NEXT_PUBLIC_APP_NAME=UserView Dashboard
NEXT_PUBLIC_APP_VERSION=1.0.0

# Fallback app variables (optional)
# NEXT_PUBLIC_APP_NAME_FALLBACK=My App
# NEXT_PUBLIC_APP_VERSION_FALLBACK=2.0.0 
